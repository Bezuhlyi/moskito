{
	driver:"org.hsqldb.jdbc.JDBCDriver",
	url:"jdbc:hsqldb:~/hsqldb-central",
	userName:"daa",
	password:"daa",
	persistenceUnitName:"hsqldbSnapshotStorage",
	
	"@mappings" : [
		{
			"producerName": "SessionCount",
            "statEntityClass": "net.anotheria.moskito.central.storage.psql.HttpSessionStatisticsEntity"
		},
		{
			"producerName": "*API,*Service*,*Action",
            "statEntityClass": "net.anotheria.moskito.central.storage.psql.ServiceStatsEntity"
		},
		{
			"producerName": "ThreadCount",
            "statEntityClass": "net.anotheria.moskito.central.storage.psql.ThreadCountStatEntity"
		},
		{
			"producerName": "MemoryPool*,Non-heap memory,Heap memory",
            "statEntityClass": "net.anotheria.moskito.central.storage.psql.MemoryPoolStatEntity"
		},
		{
			"producerName": "Runtime",
            "statEntityClass": "net.anotheria.moskito.central.storage.psql.RuntimeStatEntity"
		},
		{
			"producerName": "ThreadStates",
            "statEntityClass": "net.anotheria.moskito.central.storage.psql.ThreadStatesStatEntity"
		},
		{
			"producerName": "OS",
            "statEntityClass": "net.anotheria.moskito.central.storage.psql.OSStatEntity"
		},
		{
			"producerName": "*Filter,*Servlet,moskitoUI",
            "statEntityClass": "net.anotheria.moskito.central.storage.psql.FilterStatEntity"
		}
	]
}