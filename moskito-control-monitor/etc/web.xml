<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Moskito-Control Monitor</display-name>
	<filter>
		<filter-name>MoskitoControlUIFilter</filter-name>
		<filter-class>net.java.dev.moskito.control.monitor.shared.MoskitoControlUIFilter</filter-class>
		<init-param>
			<param-name>path</param-name>
			<param-value>/</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>MoskitoControlUIFilter</filter-name>
		<url-pattern>*.html</url-pattern>
	</filter-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<listener>
		<listener-class>net.java.dev.moskito.control.monitor.shared.MoskitoControlMonitorContextInitializer</listener-class>
	</listener>
</web-app>
